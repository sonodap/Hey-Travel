<div class='container'>
  <div class='row'>
     <div class='col-md-12'>
       <h2>ジャンル編集</h2>
       <%= form_with model: @spot_genre, url:admin_spot_genre_path, method: :patch do |f| %>
         <td><%= f.label :spot_genre_name,"ジャンル名", class: "mr-3" %></td>
         <td><%= f.text_field :name, class: "mr-5" %></td>
         <td><%= f.submit "変更を保存", class: "btn btn-primary" %></td>
       <% end %>
     </div>
  </div>
</div>